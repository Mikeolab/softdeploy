{
  "name": "test-execution-server",
  "version": "1.0.0",
  "description": "Real test execution server for SoftDeploy",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "vitest run",
    "test:ci": "vitest run",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:api": "vitest run --config vitest.api.config.js",
    "test:api:watch": "vitest --config vitest.api.config.js",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "seed:sample": "node scripts/seed-sample-data.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "cypress": "^13.6.0",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "puppeteer": "^24.18.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.10",
    "eslint": "^9.33.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.4",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "keywords": [
    "testing",
    "automation",
    "cypress",
    "playwright",
    "selenium",
    "puppeteer"
  ],
  "author": "SoftDeploy",
  "license": "MIT"
}
